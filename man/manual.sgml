 page = panda_newpage (document, panda_pagesize_a4);
 
 panda_setlinestart (page, 100, 200);
 panda_setfillcolor (page, 100, 200, 300);
 panda_addlinesegment (page, 200, 200);
 panda_addlinesegment (page, 250, 300);
 panda_closeline (page);
 panda_endline (page);
